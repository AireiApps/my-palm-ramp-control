<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.airei.milltracking.mypalm.mqtt.lrc.ui.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_mqttConfigFragment"
            app:destination="@id/mqttConfigFragment" />
    </fragment>
    <fragment
        android:id="@+id/mqttConfigFragment"
        android:name="com.airei.milltracking.mypalm.mqtt.lrc.ui.MqttConfigFragment"
        android:label="fragment_mqtt_config"
        tools:layout="@layout/fragment_mqtt_config" >
        <action
            android:id="@+id/action_mqttConfigFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>